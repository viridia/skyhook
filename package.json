{
  "name": "skyhook-ui",
  "version": "1.4.1",
  "description": "Themable UI framework built with styled-components.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run build-dist && npm run build-storybook",
    "build-dist": "tsc --project ./tsconfig.dist.json",
    "build-storybook": "build-storybook -c .storybook -o docs/storybook",
    "clean": "rm -r dist",
    "gen-icons": "cd lib/icons && npx @svgr/cli --ext tsx --out-dir=../icons-compiled *.svg",
    "storybook": "start-storybook --port 4321"
  },
  "keywords": [
    "styled-components",
    "react",
    "widgets"
  ],
  "author": "Talin",
  "license": "MIT",
  "dependencies": {
    "bind-decorator": "^1.0.11",
    "classnames": "^2.2.6",
    "polished": "^3.2.0",
    "react-overlays": "^1.2.0",
    "react-transition-group": "^2.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "@types/classnames": "^2.2.7",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-overlays": "^1.1.2",
    "@types/react-transition-group": "^2.0.16",
    "@types/storybook-addon-jsx": "^5.4.3",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "^4.1.12",
    "babel-loader": "^8.0.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "storybook-addon-jsx": "^7.1.0",
    "styled-components": "^4.2.0",
    "typescript": "^3.4.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "styled-components": ">=4.1.0"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viridia/skyhook.git"
  },
  "bugs": {
    "url": "https://github.com/viridia/skyhook/issues"
  },
  "homepage": "https://github.com/viridia/skyhook#readme"
}
